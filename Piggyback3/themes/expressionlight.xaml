              </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="FocussedOff">
              <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="FocusVisualElement"
                                             Storyboard.TargetProperty="(UIElement.Opacity)">
                <SplineDoubleKeyFrame KeyTime="00:00:00.3000000" Value="0"/>
              </DoubleAnimationUsingKeyFrames>
            </Storyboard>
          </FrameworkTemplate.Resources>
          <Grid SnapsToDevicePixels="true" Margin="1,1,1,1">
            <Rectangle x:Name="Background" IsHitTestVisible="false" Opacity="0.25" Fill="{StaticResource NormalBrush}"
                       RadiusX="1" RadiusY="1"/>
            <Rectangle x:Name="HoverRectangle" IsHitTestVisible="false" Opacity="0" Fill="{StaticResource NormalBrush}"
                       RadiusX="1" RadiusY="1"/>
            <Rectangle x:Name="SelectedRectangle" IsHitTestVisible="false" Opacity="0"
