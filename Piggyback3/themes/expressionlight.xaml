                              VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" Margin="5,2,0,2"/>
            <Rectangle x:Name="FocusVisualElement" Stroke="{StaticResource HoverShineBrush}" StrokeThickness="1"
