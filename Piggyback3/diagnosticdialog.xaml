<Window x:Class="ActivityBrowser.DiagnosticDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Diagnostic Code" Height="165" Width="503" WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize">
  <Grid>
    <Button Click="btnCancel_Click" Name="btnCancel" Content="Cancel" Height="23" HorizontalAlignment="Left"
            Margin="394,91,0,0" VerticalAlignment="Top" Width="75" IsCancel="true"/>
    <Button Click="btnOk_Click" Name="btnOk" Content="Ok" Height="23" HorizontalAlignment="Left"
            Margin="313,91,0,0" VerticalAlignment="Top" Width="75" IsDefault="true"/>
    <Label Name="label1" Content="Please enter the diagnostic code." Height="28" HorizontalAlignment="Left"
           Margin="12,12,0,0" VerticalAlignment="Top"/>
    <TextBox Name="txtDiagCode" Height="23" HorizontalAlignment="Left" Margin="12,46,0,0"
             VerticalAlignment="Top" Width="457" MaxLength="64"/>
  </Grid>
</Window>